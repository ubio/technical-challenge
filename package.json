{
    "name": "technical-challenge",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "main": "out/bin/serve.js",
    "scripts": {
        "start": "node out/bin/serve",
        "dev": "npm run clean && tsc -w",
        "lint": "eslint --ext=.js,.ts --cache .",
        "clean": "rm -rf out",
        "compile": "npm run clean && tsc",
        "test": "NODE_ENV=test mocha",
        "docs": "npm run docs:openapi && npm run docs:api",
        "docs:openapi": "generate-openapi > openapi.json",
        "docs:api": "generate-docs > API.md",
        "preversion": "npm run lint && npm run compile && npm run docs && git add openapi.json API.md",
        "postversion": "git push origin $(git rev-parse --abbrev-ref HEAD) --tags"
    },
    "author": "UBIO",
    "license": "ISC",
    "dependencies": {
        "@types/koa": "^2.14.0",
        "@ubio/framework": "^15.11.1",
        "dotenv": "^16.4.1",
        "koa": "^2.15.0",
        "mesh-ioc": "^3.2.0",
        "reflect-metadata": "^0.1.13",
        "mongodb": "^4.17.2"
    },
    "devDependencies": {
        "@nodescript/eslint-config": "^1.1.0",
        "@types/node": "^20.11.13",
        "eslint": "^8.56.0",
        "mocha": "^10.2.0",
        "npm-run-all": "^4.1.5",
        "typescript": "^5.3.3"
    }
}
